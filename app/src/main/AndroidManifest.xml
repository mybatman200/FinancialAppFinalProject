<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tringuyen.financialappfinalproject">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Name_Initial_Screen"
            android:label="@string/title_activity_name__initial__screen" />
        <activity android:name=".activity_income_question" />
        <activity android:name=".activity_saving_goal_question" />
        <activity android:name=".activity_user_profile" />
        <activity android:name=".profile_user_edit" />
        <activity android:name=".profile_user_summary" />
        <activity android:name=".saving_plan" />
        <activity android:name=".saving_plan_2" />
        <activity android:name=".saving_plan_3" />
        <activity android:name=".activity_add_daily" />
        <activity android:name=".activity_deduct_daily" />
        <activity android:name=".Recuring" />
        <activity android:name=".Recurring_2" />
        <activity android:name=".Recurring_3"></activity>
    </application>


</manifest>